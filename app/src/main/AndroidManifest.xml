<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cradle.rancune.learningandroid">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/MainTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".aidl.client.BookManagerClientActivity"
            android:label="@string/activity_aidl" />

        <service
            android:name=".aidl.server.BookManagerService"
            android:process=":remote" />

        <activity
            android:name=".opengl.ui.OpenGL01Activity"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".opengl.ui.OpenGL02Activity"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".opengl.ui.OpenGL03Activity"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".opengl.ui.OpenGL04Activity"
            android:theme="@style/AppTheme" />

        <activity android:name=".opengl.ui.OpenGL05Activity"
            android:theme="@style/AppTheme"/>


    </application>

</manifest>